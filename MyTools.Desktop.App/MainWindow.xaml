<Window x:Class="MyTools.Desktop.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyTools.Desktop.App"
        mc:Ignorable="d"
        AllowsTransparency="True" 
        WindowStyle="None" 
        ResizeMode="NoResize" 
        Topmost="True"
        Title="MainWindow" Height="500" Width="500" Loaded="Window_Loaded">

    <Window.Background>
        <SolidColorBrush Opacity="0" Color="White"/>
    </Window.Background>
    
    <Grid>
        <Ellipse Fill="#FF9F9FA4" HorizontalAlignment="Left" Height="100" Margin="390,390,0,0" Stroke="Black" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
